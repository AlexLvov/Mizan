mixin b-isotope-1()

  .b-isotope(class!=attributes.class)
    .container
      .row
        .col-xs-12
          ul.b-isotope-filter.list-inline
            li.current
              a(href="", data-filter="*") all works
            li
              a(href="", data-filter=".branding") branding
