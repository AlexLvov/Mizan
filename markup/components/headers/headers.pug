mixin searchModal()

  // ==========================
  // SEARCH MODAL
  // ==========================

  .header-search.open-search
    .container
      .row
        .col-sm-8.col-sm-offset-2.col-xs-10.col-xs-offset-1
          .navbar-search
            form.search-global
              input.search-global__input(type="text", placeholder="Type to search", autocomplete="off", name="s", value="")
              button.search-global__btn
                i.icon.stroke.icon-Search
              .search-global__note Begin typing your search above and press return to search.
    button.search-close.close(type="button")
      i.fa.fa-times



